<h2>Login</h2>

<%= form_for :user, url: sessions_path do |f| %>
  <%= f.label :email, "Email: " %>
  <%= f.text_field :email %>
  <%= f.label :password, "Password: " %>
  <%= f.text_field :password, type: "password" %>
  <%= f.submit "Login" %>
<% end %>
<br>
<%= link_to "Don't have an account?  Sign up here", signup_path %>
